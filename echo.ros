(defpackage :ros.script.echo.3922517100
  (:use :cl))
(in-package :ros.script.echo.3922517100)

(defun main (&rest argv)
  (declare (ignorable argv))
  (loop for line = (read-line *standard-input* nil :eof)
        until (eq line :eof)
        do (cl-maelstrom:parse-input line)))
